<h1>Vining Plants in Containers</h1>

    <ul> 
        <% for(let i = 4; i < allPlants.length; i++) { %>
             <li><h2><%=allPlants[4].variety.recommendation %></h2></li>
             <img src=
                "<%=allPlants[4].variety.img %>" height="200"/>
           
            <h2>Container and Supports: </h2>
            <li><%=allPlants[4].supports %> </li>

            <h2>Planting Features:</h2>
            <li><%=allPlants[4].variety.plant.soilTemperature %></li>
            <li><%=allPlants[4].variety.light %></li>
            <li>
                <a href="<%=allPlants[4].variety._links.url %>"> Shop Here </a>
            </li>
                
        <% } %>
    </ul>
<div>
    <buttonn class="btn btn-primary" > More Gardeing Tips</button>
</div>
<div>
    <a class="btn btn-success" href="/new"> Add Vining Plants</a>
</div>
<div>
    <a class="btn btn-warning" href="/edit"> Edit Vining Plants</a>
</div>
<div>
    <a class="btn btn-danger" href="/delete"> Delete Vining Plants</a>
</div>
